{
  "name": "kwizzad-web-sdk",
  "private": true,
  "version": "0.7.1",
  "description": "Shows Kwizzad campaigns embedded in a web site.",
  "main": "public/index.js",
  "scripts": {
    "start": "NODE_ENV=development ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --content-base public/ --hot --inline --devtool eval --host 0.0.0.0",
    "build": "./node_modules/webpack/bin/webpack.js -p && mkdir -p public/public && mv public/bundle.js public/public/bundle.js && mkdir -p versions && rm -f versions/current.tar.gz && tar --exclude 'public/versions' -czf versions/current.tar.gz public && (echo `date +\"%Y-%m-%d-%H.%M.%S.%3N\"`-${GIT_COMMIT} > versions/VERSION) && cp versions/current.tar.gz versions/`cat versions/VERSION`.tar.gz",
    "deploy-dev": "node s3-upload.js -d develop",
    "deploy-feature": "node s3-upload.js -d feature",
    "test": "node -v",
    "preversion": "npm test",
    "version": "npm run build",
    "postversion": "npm test"
  },
  "author": "TVSmiles",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2015-minimal": "^2.1.0",
    "babel-preset-react": "^6.11.1",
    "css-loader": "^0.25.0",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-import-resolver-meteor": "^0.2.4",
    "eslint-plugin-import": "^1.10.3",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-meteor": "^3.6.0",
    "eslint-plugin-react": "^6.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "jquery": "^3.1.1",
    "js-logger": "^1.3.0",
    "lodash": "^4.17.4",
    "mixpanel-browser": "^2.10.0",
    "moment": "^2.17.1",
    "nib": "^1.1.2",
    "node-uuid": "^1.4.7",
    "optimist": "^0.6.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.5",
    "react-lite": "^0.15.26",
    "style-loader": "^0.13.1",
    "stylus-loader": "^2.3.1",
    "uglify-loader": "^1.3.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  },
  "devDependencies": {
    "eslint-import-resolver-webpack": "^0.6.0",
    "webpack-bundle-analyzer": "^1.4.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "settings": {
      "import/resolver": "webpack"
    },
    "extends": [
      "airbnb"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "allowImportExportEverywhere": true,
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "no-console": [
        0
      ]
    }
  }
}
